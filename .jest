{
	"testRegex": "(/test/.*-test|(-|\\.)(test|spec))\\.(js)$",
	"collectCoverage": true,
	"setupFiles": ["./test/before-test-start.js"],
	"setupTestFrameworkScriptFile": "./test/before-test-setup.js",
	"coverageReporters": ["json", "lcov", "text", "text-summary"],
	"coverageThreshold": {
		"global": {
			"statements": 50,
			"branches": 70,
			"functions": 50,
			"lines": 50
		}
	},
	"testEnvironment": "node",
	"verbose": true
}