{
  "name": "billdone",
  "version": "1.0.0",
  "description": "API for a financial control",
  "main": "index.js",
  "scripts": {
    "checkdep": "depcheck --ignores nodejs-dashboard && snyk auth 24e5f65a-a5a5-4cc3-b2f0-ea85f87f06ef && snyk test",
    "node-debug": "DEBUG=express* babel-node index.js",
    "chrome-debug": "babel-node --debug-brk --inspect index.js",
    "pretest": "yarn lint && yarn checkdep",
    "test": "NODE_ENV=test nyc mocha",
    "test:unit": "NODE_ENV=test mocha test/unit",
    "test:integration": "NODE_ENV=test mocha test/integration",
    "start": "babel-node index.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "metric": "nodejs-dashboard -- babel-node index.js"
  },
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.22.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "joi": "^10.2.1",
    "mongodb": "^2.2.22",
    "morgan": "^1.8.1"
  },
  "repository": "git@github.com:BrunoCamargos/billdone.git",
  "author": "Bruno Camargos <bruno.camargos@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-istanbul": "^4.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "depcheck": "^0.6.7",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "6.9.0",
    "joi-assert": "^0.0.3",
    "mocha": "^3.2.0",
    "nodejs-dashboard": "^0.4.0",
    "nyc": "^10.1.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "snyk": "^1.25.2",
    "supertest": "^3.0.0"
  }
}
